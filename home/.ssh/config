Host *
	ServerAliveInterval 120
Host github.com
  HostName ssh.github.com
  Port 443
Host bastion-tl-dev
  ProxyCommand zsh -c 'source ~/.zshrc && nc $(ec2host bastion) %p'
  User akira-uchihara
  Port 50022
Host bastion-tl-prd
  ProxyCommand zsh -c 'source ~/.zshrc && nc $(ec2host bastion) %p'
  User akira-uchihara
  Port 22
Host bastion-tl-dev-dbproxy
  ProxyCommand zsh -c 'source ~/.zshrc && nc $(ec2host bastion) %p'
  User akira-uchihara
  Port 50022
  LocalForward 13306 db.timeline-media.local:3306
Host bastion-tl-prd-dbproxy
  ProxyCommand zsh -c 'source ~/.zshrc && nc $(ec2host bastion) %p'
  User akira-uchihara
  Port 22
  LocalForward 23306 db.timeline-media.local:3306
